generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model Users {
  UserID     Int       @id(map: "PK__Users__1788CCACF1CB46F6") @default(autoincrement())
  FirstName  String    @db.NVarChar(100)
  SecondName String    @db.NVarChar(100)
  Email      String    @unique(map: "UQ__Users__A9D105347E6A8FF5") @db.NVarChar(255)
  Password   String    @db.NVarChar(255)
  HotelID    Int?
  CreatedAt  DateTime? @default(now(), map: "DF__Users__CreatedAt__4E88ABD4") @db.DateTime
}
