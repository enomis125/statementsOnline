generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model Users {
  UserID           Int       @id(map: "PK__Users__1788CCAC117E4C7A") @default(autoincrement())
  FirstName        String    @db.NVarChar(100)
  SecondName       String    @db.NVarChar(100)
  Email            String    @unique(map: "UQ__Users__A9D1053459291FCD") @db.NVarChar(255)
  Password         String    @db.NVarChar(255)
  ConnectionString String?   @db.NVarChar(255)
  CreatedAt        DateTime? @default(now(), map: "DF__Users__CreatedAt__403A8C7D") @db.DateTime
}
